{
  "name": "c-store-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "start": "node dist/src/server.js",
    "db:dev:migrate": "npx prisma migrate dev --name init",
    "db:production:push": "npx prisma db push",
    "db:production:migrate": "npx prisma migrate deploy",
    "docker:up": "docker compose up -d cstore-dev-db cstore-api",
    "docker:down": "docker compose down"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.npm 6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}

{
  "name": "c-store-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "db:dev:migrate": "npx prisma migrate dev --name init",
    "db:dev:up": "docker compose up -d cstore-dev-db",
    "db:dev:down": "docker compose down",
    "db:production:push": "npx prisma db push",
    "db:production:migrate": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  }
}
